import java.util.*;
class Main {
    static class Tree{
        Tree left;
        Tree right;
        int data;
        public Tree(int data){
            this.data = data;
            this.left = null;
            this.right = null;
        }
    }
    
    public static void printinorderBT(Tree root){
        if(root == null) return;
        
        printinorderBT(root.left);
        System.out.print(root.data + " ");
        printinorderBT(root.right);
    }
    public static void printpreorderBT(Tree root){
        if(root == null) return;
        
        System.out.print(root.data + " ");
        printpreorderBT(root.left);
        printpreorderBT(root.right);
    }
    public static void printpostorderBT(Tree root){
        if(root == null) return;
        
        printpostorderBT(root.left);
        printpostorderBT(root.right);
        System.out.print(root.data + " ");
    }
    public static void buildBT(){
        Tree root = new Tree(1);
        root.left = new Tree(2);
        root.left.left = new Tree(3);
        root.left.right = new Tree(4);
        root.right = new Tree(5);
        root.right.left = new Tree(6);
        root.right.right = new Tree(7);
        
        System.out.print("Inorder traversal: ");
        printinorderBT(root);
        System.out.println();
        System.out.print("Preorder traversal: ");
        printpreorderBT(root);
        System.out.println();
        System.out.print("Postorder traversal: ");
        printpostorderBT(root);
    }
    public static void main(String[] args) {
        int[] arr = {1,2,3,4,5};
        buildBT();
    }
}
