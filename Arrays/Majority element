class Solution {
    public int majorityElement(int[] nums) {
        int lead = 0;
        int majority = 0;
        for(int num: nums){
            if(lead == 0){
                majority = num;
                lead=1;
            }else if(num == majority){
                lead++;
            }else{
                lead--;
            }
        }
        return majority;
    }
}
